{
  "extends": "@calcom/tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": true,
    "noImplicitAny": false,
    "paths": {
      "@lib/*": ["lib/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "../../packages/types/*.d.ts",
    "../../packages/types/next-auth.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": ["node_modules"]
}
